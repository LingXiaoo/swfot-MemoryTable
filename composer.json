{
  "name": "lingxiao/swoft-memory-table",
  "type": "library",
  "keywords": [
    "swoole",
    "swoft",
    "swoft-component"
  ],
  "description": "swoft-memory-table component for swoft framework",
  "license": "Apache-2.0",
  "require": {
    "php": ">7.1",
    "swoft/framework": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Lingxiao\\Swoft\\MemoryTable\\": "src/"
    }
  },
  "require-dev": {
    "swoft/swoole-ide-helper": "dev-master",
    "phpunit/phpunit": "^7.5"
  },
  "autoload-dev": {
    "psr-4": {
      "Lingxiao\\Swoft\\MemoryTable\\Test\\Unit\\": "test/unit/",
      "Lingxiao\\Swoft\\MemoryTable\\Test\\Testing\\": "test/testing/",
      "SwoftTest\\Unit\\": "vendor/swoft/framework/test/unit/",
      "SwoftTest\\Testing\\": "vendor/swoft/framework/test/testing/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
